//Identifying sum of two value given by the user.

#include<stdio.h>
int main()
{
    printf("Sum Calculation\n\n");
    
    int value1, value2, sum;
    
    printf("\nFirst Value:");
    scanf("%d", &value1);
    
    printf("\nSecond Value:");
    scanf("%d", &value2);
    
    sum = value1 + value2;
    
    printf("Sum: %d", sum);
    
    return 0;
    
}
